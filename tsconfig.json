{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
     
     
    }
  }
}
